Host bitbucket.org
  HostName bitbucket.org
  IdentityFile ~/.ssh/bitbucket/id_rsa.bitbucket.org
  User git
  Port 22
  TCPKeepAlive yes
  IdentitiesOnly yes

Host github.com
  HostName github.com
  IdentityFile ~/.ssh/github/id_rsa.github.com
  User git
  Port 22
  TCPKeepAlive yes
  IdentitiesOnly yes
